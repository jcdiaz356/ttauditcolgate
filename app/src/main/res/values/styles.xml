<resources>

    <!-- Base application theme. -->
   <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
       <!--<item name="android:actionBarStyle">@style/MyActionBar</item>-->
       <!--<item name="android:windowBackground">@color/colo_buttom_naranja_pressed</item>-->
       <item name="actionBarStyle">@style/MyActionBar</item>
       <item name="android:popupMenuStyle">@style/PopupMenu</item>
    </style>


    <!-- Base application theme. -->
    <style name="AppThemeAdminPanel" parent="@android:style/Theme.Holo.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <!--<item name="actionBarStyle">@style/MyActionBar2</item>-->
        <item name="android:actionBarStyle">@style/MyActionBar2</item>
        <item name="android:popupMenuStyle">@style/PopupMenu</item>
        <item name="android:actionBarWidgetTheme">@style/Theme.stylingactionbar.widget</item>


    </style>
    <!--Tema para los interiores-->

    <style name="AppThemeInterior" parent="android:Theme.Holo.Light.DarkActionBar">
        <item name="android:actionBarStyle">@style/AppTheme_ActionBar</item>
        <item name="android:actionBarSize">56dp</item>

        <item name="android:homeAsUpIndicator">@drawable/ic_ab_up_compat</item>
        <item name="android:actionOverflowButtonStyle">@style/AppTheme_ActionBar_Overflow</item>
    </style>

    <style name="AppTheme_ActionBar" parent="android:Widget.Holo.ActionBar.Solid">
        <item name="android:background">@color/color_base</item>

        <item name="android:titleTextStyle">@style/AppTheme_ActionBar_Title</item>
    </style>

    <style name="AppTheme_ActionBar_Title" parent="android:TextAppearance.Holo.Widget.ActionBar.Title">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">20sp</item>
    </style>

    <style name="AppTheme_ActionBar_Overflow" parent="android:Widget.ActionButton.Overflow">

    </style>
    <!-- Fin Tema para los interiores-->




    <style name="Theme.stylingactionbar.widget" parent="@android:style/Theme.Holo">
        <item name="android:popupMenuStyle">@style/PopupMenu</item>
        <!--<item name="android:dropDownListViewStyle">@style/DropDownListView</item>-->
        <!--<item name="android:spinnerStyle">@style/DropDownNav</item>-->
        <!--<item name="android:editTextBackground">@drawable/edit_text_holo_dark</item>-->
        <!--<item name="android:textColorHint">@color/hint</item>-->
    </style>
    <!-- ActionBar styles -->
    <style name="MyActionBar2"
        parent="@android:style/Widget.Holo.Light.ActionBar.Solid.Inverse">
        <item name="android:background">@color/color_base</item>
    </style>

    <style name="MyActionBar3"
        parent="@android:style/Theme.Black.NoTitleBar">

    </style>

    <!--<application android:theme="@style/CustomActionBarTheme" ... />-->

    <style name="TextLogin" parent="@android:style/TextAppearance.Medium">
        <item name="android:textColor">#ff000000</item>
        <item name="android:textSize">@dimen/general</item>
        <item name="android:drawablePadding">5dp</item>
        <item name="android:background">@drawable/edittext_selector</item>
    </style>
    <style name="BottonBase" >
        <item name="android:textColor">#ffffffff</item>
        <item name="android:textSize">@dimen/general</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:background">@drawable/bottom_base</item>
    </style>

    <style name="BottonCloseAudit" >
        <item name="android:textColor">#ffffffff</item>
        <item name="android:textSize">@dimen/general</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:background">@drawable/botton_close_audit</item>
    </style>




    <style name="BottonButton" >
        <item name="android:textColor">@color/color_base</item>
        <item name="android:textSize">14sp</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:background">@drawable/bottom_button</item>
    </style>

    <style name="ButtonCerrar" >
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">20sp</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:background">#c60000</item>
    </style>


    <style name="BottonBaseRutas" >
        <item name="android:textColor">#ffffffff</item>
        <item name="android:textSize">20sp</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:background">@drawable/bottom_base</item>
    </style>
    <style name="MyActionBar" parent="Widget.AppCompat.Light.ActionBar.Solid.Inverse">
        <item name="android:background">@color/color_base</item>
        <item name="background">@color/color_base</item>
    </style>
    <style name="MyMenu" parent="@style/Widget.AppCompat.Light.PopupMenu">
        <item name="android:background">@color/color_base</item>
        <item name="background">@color/color_base</item>
    </style>
    <!--<style name="PopupMenu" parent="Widget.AppCompat.PopupMenu">-->
        <!--<item name="android:popupBackground">#ffff7a2f</item>-->
    <!--</style>-->
    <!--<style name="PopupMenu" parent="@android:style/Widget.Holo.Light">-->
        <!--<item name="android:popupBackground">#ffff4141</item>-->
    <!--</style>-->
    <style name="PopupMenu" parent="@android:style/Widget.Holo.Light.ListPopupWindow">
        <item name="android:popupBackground">@drawable/list_selector</item>
    </style>
</resources>
